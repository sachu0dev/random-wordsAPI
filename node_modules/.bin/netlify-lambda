#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sachudev/code/random-wordsAPI/node_modules/.pnpm/netlify-lambda@2.0.16/node_modules/netlify-lambda/bin/node_modules:/home/sachudev/code/random-wordsAPI/node_modules/.pnpm/netlify-lambda@2.0.16/node_modules/netlify-lambda/node_modules:/home/sachudev/code/random-wordsAPI/node_modules/.pnpm/netlify-lambda@2.0.16/node_modules:/home/sachudev/code/random-wordsAPI/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sachudev/code/random-wordsAPI/node_modules/.pnpm/netlify-lambda@2.0.16/node_modules/netlify-lambda/bin/node_modules:/home/sachudev/code/random-wordsAPI/node_modules/.pnpm/netlify-lambda@2.0.16/node_modules/netlify-lambda/node_modules:/home/sachudev/code/random-wordsAPI/node_modules/.pnpm/netlify-lambda@2.0.16/node_modules:/home/sachudev/code/random-wordsAPI/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../netlify-lambda/bin/cmd.js" "$@"
else
  exec node  "$basedir/../netlify-lambda/bin/cmd.js" "$@"
fi
