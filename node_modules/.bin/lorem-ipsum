#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sachudev/code/random-wordsAPI/node_modules/.pnpm/lorem-ipsum@2.0.8/node_modules/lorem-ipsum/dist/bin/node_modules:/home/sachudev/code/random-wordsAPI/node_modules/.pnpm/lorem-ipsum@2.0.8/node_modules/lorem-ipsum/dist/node_modules:/home/sachudev/code/random-wordsAPI/node_modules/.pnpm/lorem-ipsum@2.0.8/node_modules/lorem-ipsum/node_modules:/home/sachudev/code/random-wordsAPI/node_modules/.pnpm/lorem-ipsum@2.0.8/node_modules:/home/sachudev/code/random-wordsAPI/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sachudev/code/random-wordsAPI/node_modules/.pnpm/lorem-ipsum@2.0.8/node_modules/lorem-ipsum/dist/bin/node_modules:/home/sachudev/code/random-wordsAPI/node_modules/.pnpm/lorem-ipsum@2.0.8/node_modules/lorem-ipsum/dist/node_modules:/home/sachudev/code/random-wordsAPI/node_modules/.pnpm/lorem-ipsum@2.0.8/node_modules/lorem-ipsum/node_modules:/home/sachudev/code/random-wordsAPI/node_modules/.pnpm/lorem-ipsum@2.0.8/node_modules:/home/sachudev/code/random-wordsAPI/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lorem-ipsum/dist/bin/lorem-ipsum.bin.js" "$@"
else
  exec node  "$basedir/../lorem-ipsum/dist/bin/lorem-ipsum.bin.js" "$@"
fi
